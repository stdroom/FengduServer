<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.demo.model.models.Image" table="Image" lazy="false">  

		<id name="id" column="id" type="integer" >
		  <generator class="native" />
		</id>
		
		<property name="contextPath" column="contextPath" type="string" not-null="false"/>
		<property name="contextHtml" column="contextHtml" type="string" not-null="false"/>	
		<property name="currentPage" column="currentPage" type="integer" not-null="false"/>
		<property name="currentPageUrl" column="currentPageUrl" type="string" not-null="false"/>
		<property name="baseUrl" column="baseUrl" type="string" not-null="false"/>
		<property name="exception" column="exception" type="string" not-null="false"/>
		<property name="imgPaths" column="imgPaths" type="string" not-null="false"/>
		<property name="srcImgpaths" column="srcImgpaths" type="string" not-null="false"/>
		<property name="yunImgPaths" column="yunImgPaths" type="string" not-null="false"/>
		<property name="thumbSrc" column="thumbSrc" type="string" not-null="false"/>
		<property name="thumbYun" column="thumbYun" type="string" not-null="false"/>
		<property name="thumbNail" column="thumbNail" type="string" not-null="false"/>
		<property name="cata_id" column="cata_id" type="integer" not-null="false"/>
		<property name="hasNext" column="hasNext" type="integer" not-null="false"/>
		<property name="isParseError" column="isParseError" type="integer" not-null="false"/>
		<property name="width" column="width" type="integer" not-null="false"/>
		<property name="height" column="height" type="integer" not-null="false"/>
		<property name="zan" column="zan" type="integer" not-null="false"/>
		<property name="favor" column="favor" type="integer" not-null="false"/>
		<property name="pageNum" column="pageNum" type="integer" not-null="false"/>
		<property name="title" column="title" type="string" not-null="false"/>

    </class>  
</hibernate-mapping> 